<MyGrid>:
	GridLayout:
		cols:1
		size:root.width, root.height
		Label:
			text:"Choose the detection model"
		
		GridLayout:
			cols:2 

			Label:
				text:"Hog(Recommended)"

			CheckBox:
				active:True	
			
			Label:
				text:"CNN"

			CheckBox:
				active:False

		Button:
			text:"Next"
			on_press: root.btn()
